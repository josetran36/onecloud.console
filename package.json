{
  "name": "one-cloud-angular",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "dev": "ng serve --o",
    "build-qa": "set NODE_OPTIONS=--openssl-legacy-provider && ng build --configuration=qa",
    "build-dev": "set NODE_OPTIONS=--openssl-legacy-provider && ng build --configuration=dev",
    "build-demo": "set NODE_OPTIONS=--openssl-legacy-provider && ng build --configuration=demo",
    "build-staging": "set NODE_OPTIONS=--openssl-legacy-provider && ng build --configuration=staging",
    "build": "set NODE_OPTIONS=--openssl-legacy-provider && ng build --configuration=production",
    "test": "set NODE_OPTIONS=--openssl-legacy-provider && ng test",
    "test-local": "set NODE_OPTIONS=--openssl-legacy-provider && ng test --browsers=Chrome --reporters=kjhtml",
    "test-single-headless": "set NODE_OPTIONS=--openssl-legacy-provider && ng test --browsers=PhantomJS --reporters=progress,junit",
    "test-cicd": "set NODE_OPTIONS=--openssl-legacy-provider && ng test --no-watch --no-progress --code-coverage --browsers=ChromeHeadlessCI --reporters=kjhtml",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^12.2.7",
